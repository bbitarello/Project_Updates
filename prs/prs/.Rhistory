dt=do.call(rbind, lapply(list.files(pattern="*.csv"), function(X) fread(X, sep=",", header=T)))
my_plot<-ggplot(dt, aes(x=Method,y=`Variance Explained (%)`, colour=`Discovery Cohort`, shape=`Test set`),) +
